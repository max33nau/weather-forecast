    "webdriver-manager": "^10.0.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular": "^1.5.6",
    "angular-ui-router": "^0.3.0"
  }
