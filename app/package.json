{
  "name": "front-end-weather-forecast",
  "version": "1.0.0",
  "description": "The front end of my weather-forecast app",
  "main": "src/app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "eval $(cat .env) webpack-dev-server --inline --hot",
    "build": "eval $(cat .env) webpack",
    "update-webdriver": "webdriver-manager update --standalone",
    "webdriver": "webdriver-manager start",
    "e2e": "protractor",
    "unit": "karma start"
  },
  "keywords": [
    "AngularJS",
    "bootstrap",
    "webpack",
    "weather"
  ],
  "author": "Max Jacobsen",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "devDependencies": {
    "angular-mocks": "^1.5.6",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "jasmine": "^2.4.1",
    "jshint": "^2.9.2",
    "jshint-loader": "^0.8.3",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "protractor": "^3.3.0",
    "sass-loader": "^3.2.0",
